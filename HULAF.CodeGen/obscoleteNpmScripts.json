{
  "concat-dto-library": "cd contract && swagger-merger -i ./dto-library/schemas.yaml -o ./hulaf-dto-library.yaml",
  "generate-dto-library": "nswag run ./contract/hulaf-dto-library-config.json",
  "concat-persons-controller": "cd contract && swagger-merger -i ./person-controller/controller.yaml -o ./person-controller.yaml",
  "generate-persons-controller": "nswag run ./contract/person-controller-config.json",
  "generate-persons-client": "nswag run ./contract/person-client-config.json",
  "concat-location-controller": "cd contract && swagger-merger -i ./location-controller/controller.yaml -o ./location-controller.yaml",
  "generate-location-controller": "nswag run ./contract/location-controller-config.json",
  "generate-location-client": "nswag run ./contract/location-client-config.json"
}
